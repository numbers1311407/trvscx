doctype 5
html.no-js(lang="en")
  head
    title= title
    meta(charset="utf8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href='http://fonts.googleapis.com/css?family=Oswald|Yanone+Kaffeesatz|Oxygen|Play|Fjalla+One', rel='stylesheet', type='text/css')
    != assets.tag('/css/screen.css')
    != assets.tag('/js/modernizr.js')
  body
    block content
    block bottom
      != assets.tag('/js/vendor.js')
      != assets.tag('/js/bundle.js')
    include includes/ga
